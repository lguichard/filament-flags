{
    "name": "andromede/filament-flags",
    "description": "A Laravel Filament component to display countries or languages by flags",
    "keywords": ["andromede", "laravel", "filament", "filament-flags"],
    "homepage": "https://github.com/lguichard/filament-flags",
    "license": "MIT",
    "authors": [
        {
            "name": "Lionel Guichard",
            "email": "lionel.guichard@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "filament/filament": "^3.0",
        "filament/forms": "^3.0",
        "filament/tables": "^3.0",
        "illuminate/contracts": "^10.0",
        "spatie/laravel-package-tools": "^1.14.0"
    },
    "autoload": {
        "psr-4": {
            "Andromede\\FilamentFlags\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Andromede\\FilamentFlags\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Andromede\\FilamentFlags\\FilamentFlagsServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
